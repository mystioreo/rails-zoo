<h1> Animal List </h1>
<%@animals.each do |animal|%>
  <%= link_to "#{animal.name}", animal_path(animal.id)%>
    <p> <%= link_to "Delete", animal_path(animal.id), method: :delete, data: {confirm: "are you sure you want to delete #{animal.name}?"} %></p>
<%end%>
<p><%= link_to "create new animal", new_animal_path%></p>
